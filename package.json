{
  "name": "jimthedev-blog",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.2.0"
  },
  "scripts": {
    "postinstall": "npm run theme:install",
    "theme:install": "cd themes/icarus && git clone git@github.com:jimthedev/hexo-theme-icarus.git .",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "generate": "hexo generate",
    "publish": "npm run clean && npm run generate && npm run deploy",
    "server": "hexo server",
    "start": "npm run server",
    "theme:update": "cd themes/icarus  && git pull && cp _config.yml.example _config.yml"
  },
  "dependencies": {
    "hexo": "^3.1.1",
    "hexo-cli": "1.0.1",
    "hexo-deployer-git": "0.1.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-livereload": "0.2.0",
    "hexo-renderer-ejs": "^0.1.1",
    "hexo-renderer-marked": "^0.2.9",
    "hexo-renderer-stylus": "^0.3.0",
    "hexo-server": "^0.1.3"
  }
}
